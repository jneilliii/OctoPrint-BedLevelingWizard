<h4>Bed Leveling Wizard</h4>
<form class="form-horizontal" id="settings_plugin_bedlevelingwizard_form">
    <div class="row-fluid">
		<div class="well"><h5>Start GCode</h5>
			<div class="row-fluid">
                <textarea class="input-block-level" rows="3" data-bind="value: settingsViewModel.settings.plugins.bedlevelingwizard.start_gcode"></textarea>
            </div>
        </div>
    </div>
    <div class="row-fluid">
		<div class="well"><h5>End GCode</h5>
			<div class="row-fluid">
                <textarea class="input-block-level" rows="3" data-bind="value: settingsViewModel.settings.plugins.bedlevelingwizard.end_gcode"></textarea>
            </div>
        </div>
    </div>
	<div class="row-fluid">
		<div class="well"><h5>Travel Speeds</h5>
			<div class="row-fluid">
				<div class="input-append span3">
					<label for="speed_xy">X/Y Axes</label>
					<input class="input-mini text-right" type="number" id="speed_xy" data-bind="value: settingsViewModel.settings.plugins.bedlevelingwizard.speed_xy"/>
					<div class="add-on">
						<span class="input-group-text" id="basic-addon2">mm/s</span>
					</div>
				</div>
				<div class="input-append span3">
					<label for="speed_z_probe">Z Axis</label>
					<input class="input-mini text-right" type="number" id="speed_z_probe" data-bind="value: settingsViewModel.settings.plugins.bedlevelingwizard.speed_z_probe"/>
					<div class="add-on">
						<span class="input-group-text" id="basic-addon2">mm/s</span>
					</div>
				</div>
			</div>
		</div>
		<div class="well" data-bind="visible: !settingsViewModel.settings.plugins.bedlevelingwizard.use_custom_points()" style="display: none;"><h5>Offsets</h5>
			<div class="row-fluid">
				<div class="input-append span3">
					<label for="offset_xy">X/Y Axes</label>
					<input class="input-mini text-right" type="number" id="offset_xy" step="1" min="0" data-bind="value: settingsViewModel.settings.plugins.bedlevelingwizard.offset_xy"/>
					<div class="add-on">
						<span class="input-group-text" id="basic-addon2">mm</span>
					</div>
				</div>
				<div class="input-append span3">
					<label for="offset_z_travel">Z Travel Height</label>
					<input class="input-mini text-right" type="number" id="offset_z_travel" step="1" min="0" data-bind="value: settingsViewModel.settings.plugins.bedlevelingwizard.offset_z_travel"/>
					<div class="add-on">
						<span class="input-group-text" id="basic-addon2">mm</span>
					</div>
				</div>
				<div class="input-append span3">
					<label for="offset_z">Z Leveling Height</label>
					<input class="input-mini text-right" type="number" id="offset_z" step=".04" min="0" data-bind="value: settingsViewModel.settings.plugins.bedlevelingwizard.offset_z"/>
					<div class="add-on">
						<span class="input-group-text" id="basic-addon2">mm</span>
					</div>
				</div>
			</div>
		</div>
		<div class="control-group"><input type="checkbox" class="input-checkbox" style="display: inline-block;margin-bottom: 5px;" data-bind="checked: settingsViewModel.settings.plugins.bedlevelingwizard.use_custom_points"/> Use Custom Points</div>
		<div class="well" data-bind="visible: settingsViewModel.settings.plugins.bedlevelingwizard.use_custom_points()" style="display: none;"><h5>Custom Points</h5>
			<div class="row-fluid">
				<div class="span7">
					<div class="row-fluid">
						<div class="row-fluid" style="margin-bottom: 5px;">
							<div class="span4 text-center">X</div>
							<div class="span4 text-center">Y</div>
							<div class="span2 btn-group text-center" style="padding-top: 5px;">
								<button class="btn btn-mini icon-arrow-up hidden"></button>
								<button class="btn btn-mini icon-arrow-down hidden"></button>
							</div>
							<div class="span2 btn-group text-center"><button class="btn btn-mini icon-plus" data-bind="click: $root.addPoint"></button></div>
						</div>
					</div>
					<div class="row-fluid" data-bind="foreach: settingsViewModel.settings.plugins.bedlevelingwizard.custom_points">
						<div class="row-fluid" style="margin-bottom: 5px;">
							<div class="input-append span4"><input type="number" class="input-mini text-right span6" data-bind="value: point_x"/><div class="add-on"><span class="input-group-text" id="basic-addon2">mm</span></div></div>
							<div class="input-append span4"><input type="number" class="input-mini text-right span6" data-bind="value: point_y"/><div class="add-on"><span class="input-group-text" id="basic-addon2">mm</span></div></div>
							<div class="span2 btn-group text-center" style="padding-top: 5px;">
								<button class="btn btn-mini btn-primary icon-arrow-up" data-bind="click: $parent.moveUp.bind($parent, $index), disable: $index() == 0"></button>
								<button class="btn btn-mini btn-primary icon-arrow-down" data-bind="click: $parent.moveDown.bind($parent, $index), disable: ($index() === ($root.settingsViewModel.settings.plugins.bedlevelingwizard.custom_points().length - 1))"></button>
							</div>
							<div class="span2 btn-group text-center" style="padding-top: 5px;"><button class="btn btn-mini btn-danger icon-trash" data-bind="click: $root.removePoint"></button></div>
						</div>
					</div>
				</div>
				<div class="span5">
					<div class="row-fluid">
						<div class="span6" style="margin-bottom: 5px;">Z Travel Height</div>
						<div class="span6" style="margin-bottom: 5px;">Z Leveling Height</div>
					</div>
					<div class="row-fluid">
						<div class="input-append span6">
							<input class="input-mini text-right span6" type="number" step="1" min="0" data-bind="value: settingsViewModel.settings.plugins.bedlevelingwizard.offset_z_travel"/>
							<div class="add-on">
								<span class="input-group-text" id="basic-addon2">mm</span>
							</div>
						</div>
						<div class="input-append span6">
							<input class="input-mini text-right span6" type="number" step=".1" min="0" data-bind="value: settingsViewModel.settings.plugins.bedlevelingwizard.offset_z"/>
							<div class="add-on">
								<span class="input-group-text" id="basic-addon2">mm</span>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</form>
